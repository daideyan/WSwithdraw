            <!-- ngMessage: minlength -->
          </div>
          <div class="help-block ng-hide" ng-show="model.errors.new_password">
            <!-- ngRepeat: error in model.errors.new_password -->
          </div>
        </div>
      </div>
      <div class="row-md" ng-class="{ 'has-error' : (model.passwordForm.confirm_password.$touched &amp;&amp; model.passwordForm.confirm_password.$invalid) || model.errors.confirm_password }">
        <label for="confirm_password" class="col-2 control-label ng-binding">
          Confirm password
        </label>
        <div class="col-5">
          <input type="password" name="confirm_password" ng-model="model.password.confirm_password" class="form-input-text size-sm ng-pristine ng-untouched ng-empty ng-valid-validator ng-invalid ng-invalid-required" required="" ui-validate=" '$value==model.password.new_password' ">
          <div class="help-block ng-active ng-hide" ng-messages="model.passwordForm.confirm_password.$error" ng-show="model.passwordForm.confirm_password.$touched">
            <!-- ngMessage: required --><p ng-message="required" class="ng-binding ng-scope">Required</p>
            <!-- ngMessage: validator -->
          </div>
          <div class="help-block ng-hide" ng-show="model.errors.confirm_password">
            <!-- ngRepeat: error in model.errors.password -->
          </div>
        </div>
      </div>
      <div class="row-md">
        <div class="col-2"></div>
        <div class="col-5">
          <button type="submit" ng-click="model.changePassword()" ladda-button="model.submitting" data-style="slide-down" class="button primary-action size-md ladda-button ng-scope" translate="general.forms.buttons.update">Update</button>
        </div>
      </div>
    </form>
  </div>
</div>
</ng-include>
<hr class="size-lg ng-scope">

<div ng-show="!viewData.anyBankAccountsRequireDocuments" class="ng-scope">
  <!-- ngInclude: 'funding/bank-account-section.html' --><div ng-include="'funding/bank-account-section.html'" class="ng-scope"><div class="row-md funding-method-container ng-scope" data-ng-show="viewData.bankAccountsLoaded">

  <div class="col-3">
    <h4 class="inline-header-md">Bank accounts</h4>
  </div>

  <div class="col-9">

    <div data-ng-show="bankAccounts.length > 0" class="">
      <p>
        Wealthsimple investment accounts can accept fund transfers from bank accounts that are in your name and that have been verified by our compliance department. Here's what we have on file for you:
      </p>

      <table class="table-opaque">
        <thead>
          <tr>
            <th class="status-column"></th>
            <th>Bank</th>
            <th>Institution</th>
            <th>Transit</th>
            <th>Account</th>
            <th>Status</th>
          </tr>
        </thead>
        <tbody>
          <!-- ngRepeat: bankAccount in bankAccounts --><tr ng-repeat="bankAccount in bankAccounts" class="ng-scope">
            <td ng-class="['status-column', bankAccount.status_category]" class="status-column success"></td>
            <td class="ng-binding">CIBC</td>
            <td class="ng-binding">010</td>
            <td class="ng-binding">***4582</td>
            <td class="ng-binding">***8698</td>
            <td ng-switch="bankAccount.status">
              <!-- ngSwitchWhen: verified --><span ng-switch-when="verified" class="ng-scope">Verified</span><!-- end ngSwitchWhen: -->
              <!-- ngSwitchWhen: document_required -->
              <!-- ngSwitchDefault:  -->
            </td>
          </tr><!-- end ngRepeat: bankAccount in bankAccounts -->
        </tbody>
      </table>
      <p ng-if="transfers" class="ng-scope">
      <%= link_to "+ Add a Bank Account", addbank_path, :class => "table-link" %>
    </p>
    </div>

  </div>

</div></div>
</div>

