<h2 class="base-margin-bottom-x4 ng-binding ng-scope">Account settings</h2>
<!-- ngInclude: undefined --><ng-include src="'core/settings/partials/basic-info.html'" class="ng-scope"><div class="row-md ng-scope">
  <div class="col-2">
    <h4 class="inline-header-md ng-binding">Your info</h4>
  </div>
  <div class="col-10">
    <form name="model.infoForm" class="form-inline size-sm ng-pristine ng-valid ng-valid-mask ng-valid-required" novalidate="">
      <div class="row-md" ng-class="{ 'has-error' :  model.infoForm.given_name.$invalid || model.errors.given_name }">
        <label for="given_name" class="col-2 control-label ng-binding">
          First name
        </label>
        <div class="col-5">
          <input type="text" name="given_name" ng-model="user.given_name" class="form-input-text size-sm ng-pristine ng-untouched ng-valid ng-not-empty ng-valid-required" required="">
          <div class="help-block ng-inactive" ng-messages="model.infoForm.given_name.$error">
            <!-- ngMessage: required -->
          </div>
          <div class="help-block ng-hide" ng-show="model.errors.given_name">
            <!-- ngRepeat: error in model.errors.given_name -->
          </div>
        </div>
      </div>
      <div class="row-md" ng-class="{ 'has-error' :  model.infoForm.surname.$invalid || model.errors.surname }">
        <label for="surname" class="col-2 control-label ng-binding">Last name</label>
        <div class="col-5">
          <input type="text" name="surname" ng-model="user.surname" class="form-input-text size-sm ng-pristine ng-untouched ng-valid ng-not-empty ng-valid-required" required="">
          <div class="help-block ng-inactive" ng-messages="model.infoForm.surname.$error">
            <!-- ngMessage: required -->
          </div>
          <div class="help-block ng-hide" ng-show="model.errors.surname">
            <!-- ngRepeat: error in model.errors.surname -->
          </div>
        </div>
      </div>
      <div class="row-md" ng-class="{ 'has-error' :  model.infoForm.email.$invalid || model.errors.email }">
        <label for="email" class="col-2 control-label ng-binding">Email</label>
        <div class="col-5">
          <input type="text" name="email" ng-model="user.email" class="form-input-text size-sm ng-pristine ng-untouched ng-valid ng-not-empty ng-valid-required" required="">
          <div class="help-block ng-inactive" ng-messages="model.infoForm.email.$error">
            <!-- ngMessage: required -->
          </div>
          <div class="help-block ng-hide" ng-show="model.errors.email">
            <!-- ngRepeat: error in model.errors.email -->
          </div>
        </div>
      </div>
      <div class="row-md" ng-class="{ 'has-error' :  model.infoForm.phone.$invalid || model.errors.phone }">
        <label for="email" class="col-2 control-label ng-binding">
          Phone
        </label>
        <div class="col-5">
          <input type="text" name="phone" ng-model="user.phone" class="form-input-text size-sm ng-pristine ng-untouched ng-valid ng-valid-mask ng-not-empty ng-valid-required" required="" ui-mask="(999) 999-9999" ui-mask-placeholder="" ui-mask-placeholder-char="_" placeholder="(___) ___-____">
          <div class="help-block ng-inactive" ng-messages="model.infoForm.phone.$error">
            <!-- ngMessage: required -->
          </div>
          <div class="help-block ng-hide" ng-show="model.errors.phone">
            <!-- ngRepeat: error in model.errors.phone -->
          </div>
        </div>
      </div>
      <div class="row-md">
        <div class="col-2"></div>
        <div class="col-5">
          <button type="submit" ng-click="model.updateInfo()" ladda-button="model.submitting" data-style="slide-down" class="button primary-action size-md ladda-button ng-scope" translate="general.forms.buttons.update">Update</button>
        </div>
      </div>
    </form>
  </div>
</div>
</ng-include>
<hr class="size-lg ng-scope" c2cf="">
<!-- ngInclude: undefined --><ng-include src="'core/settings/partials/password.html'" class="ng-scope"><div class="row-md ng-scope">
  <div class="col-2">
    <h4 class="inline-header-md ng-binding">Password</h4>
  </div>
  <div class="col-10">
    <form name="model.passwordForm" class="form-inline size-sm ng-pristine ng-invalid ng-invalid-required ng-valid-minlength ng-valid-validator" novalidate="">
      <div class="row-md" ng-class="{ 'has-error' : (model.passwordForm.password.$touched &amp;&amp; model.passwordForm.password.$invalid) || model.errors.password }">
